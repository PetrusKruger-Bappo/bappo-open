{
  "name": "bappo-components",
  "version": "0.1.0-alpha.93",
  "description": "Cross-platform React components for building Bappo apps",
  "keywords": [
    "bappo"
  ],
  "homepage": "https://github.com/bappogroup/bappo-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bappogroup/bappo-components.git"
  },
  "license": "MIT",
  "author": "Bappo",
  "files": [
    ".flowconfig",
    "es",
    "fonts",
    "fonts.gradle",
    "glyphmaps",
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && rimraf storybook/storybook-native/node_modules/bappo-components/lib/ && cp -r lib storybook/storybook-native/node_modules/bappo-components/lib",
    "build:cjs": "rimraf lib/ && BABEL_ENV=cjs babel src/ -d lib/",
    "build:es": "rimraf es/ && BABEL_ENV=es babel src/ -d es/",
    "flow": "flow",
    "lint": "eslint src/",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "es6-error": "^4.1.1",
    "exenv": "^1.2.2",
    "fbjs": "^0.8.16",
    "lodash": "^4.17.10",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-web": "^0.9.1",
    "react-recomponent": "^0.2.0",
    "react-responsive": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "eslint": "^5.6.1",
    "eslint-config-react-app": "^3.0.3",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "git-branch-is": "^2.1.0",
    "husky": "^1.2.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react": "16.5.1",
    "react-dom": "16.5.1",
    "rimraf": "^2.6.2",
    "styled-components": "^3.0.0",
    "yargs": "^12.0.2"
  },
  "peerDependencies": {
    "react": ">=16.5.1",
    "styled-components": ">=3.2.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git-branch-is master && npm run flow && npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "rnpm": {
    "assets": [
      "fonts"
    ]
  },
  "sideEffects": [
    "./src/internals/web/addNonSelectableTextStyle.js",
    "./src/internals/web/detectResize.js",
    "./src/internals/web/ViewBase.js"
  ]
}
