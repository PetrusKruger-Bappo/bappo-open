    "create-react-class": "^15.6.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "styled-components": "^3.2.6",
    "validator": "^10.4.0"
  },
  "resolutions": {
    "@storybook/react/**/react": "16.8.3",
    "@storybook/react/**/react-dom": "16.8.3"
  }
